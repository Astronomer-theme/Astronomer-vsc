{
  "name": "astronomer",
  "displayName": "Astronomer",
  "description": "A minimal theme based in dark soft colors",
  "publisher": "halcolo",
  "license": "MIT",
  "icon": "assets/img/astronomer_logo.png",
  "version": "1.2.1-Beta",
  "engines": {
    "vscode": "^1.75.0"
  },
  "keywords": [
    "unniverse",
    "dark",
    "theme",
    "color-theme",
    "VSCode",
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jdalfons/astronomer-vsc.git"
  },
  "bugs": {
    "url": "https://github.com/jdalfons/Astronomer/issues"
  },
  "categories": [
    "Themes"
  ],
  "maintainers": [
    "Juan D. Alfonso"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Astronomer",
        "uiTheme": "vs-dark",
        "path": "./themes/astronomer.json"
      }
    ]
  },
  "homepage": "https://github.com/jdalfons/Astronomer#readme",
  "scripts": {
    "build": "node ./scripts/build.js",
    "lint": "node ./scripts/lint.js",
    "package": "vsce package -o ./bin/astronomer.vsix",
    "vscode:prepublish": "npm run build",
    "vsce-publish": "vsce publish"
  },
  "devDependencies": {
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.0",
    "tinycolor2": "^1.6.0",
    "vsce": "^2.15.0"
  }
}
